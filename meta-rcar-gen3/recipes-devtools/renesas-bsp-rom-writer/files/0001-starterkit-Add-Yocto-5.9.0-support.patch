From 7fc616c8c2d0315f924bf482d6e2314bfcaedf69 Mon Sep 17 00:00:00 2001
From: Yuya Hamamachi <yuya.hamamachi.sx@renesas.com>
Date: Wed, 12 Jan 2022 11:49:25 +0900
Subject: [PATCH 1/3] starterkit: Add Yocto 5.9.0 support

Signed-off-by: Yuya Hamamachi <yuya.hamamachi.sx@renesas.com>
---
 starterkit/linux/README           |  1 +
 starterkit/windows/rom_writer.lib | 11 ++++++-----
 starterkit/yocto/table            |  1 +
 3 files changed, 8 insertions(+), 5 deletions(-)

diff --git a/starterkit/linux/README b/starterkit/linux/README
index 5272bdf..6be7939 100644
--- a/starterkit/linux/README
+++ b/starterkit/linux/README
@@ -63,6 +63,7 @@
 	You need to go to ROM dir, and call this command from there.
 
 	-y:	write Yocto
+		5.9.0
 		5.5.0
 		5.1.0
 		4.1.0
diff --git a/starterkit/windows/rom_writer.lib b/starterkit/windows/rom_writer.lib
index 59eee17..c7c4820 100644
--- a/starterkit/windows/rom_writer.lib
+++ b/starterkit/windows/rom_writer.lib
@@ -11,11 +11,12 @@ setbaud baudrate
 ; Versions
 ;
 ;=============================
-strdim versions 4
-versions[0] = "yocto/5.5.0"
-versions[1] = "yocto/5.1.0"
-versions[2] = "yocto/4.1.0"
-versions[3] = "yocto/3.21.0"
+strdim versions 5
+versions[0] = "yocto/5.9.0"
+versions[1] = "yocto/5.5.0"
+versions[2] = "yocto/5.1.0"
+versions[3] = "yocto/4.1.0"
+versions[4] = "yocto/3.21.0"
 
 ; It doesn't support Android setting update
 ; on Windows/TeraTerm for now.
diff --git a/starterkit/yocto/table b/starterkit/yocto/table
index 6cf6ea9..f0a9a7f 100644
--- a/starterkit/yocto/table
+++ b/starterkit/yocto/table
@@ -6,6 +6,7 @@
 #	https://elinux.org/R-Car/Boards/M3SK#Flashing_firmware
 #	https://elinux.org/R-Car/Boards/M3NSK#Flashing_firmware
 #
+5.9.0:map02
 5.5.0:map02
 5.1.0:map02
 4.1.0:map01
-- 
