+++ b/arch/arm64/boot/dts/renesas/ulcb-ccpf-sk.dtsi
@@ -17,6 +17,12 @@ hdmi1_con: endpoint {
 			};
 		};
 	};
+
+	s2ram_extint {
+		compatible = "shimafuji,ccpf-sk";
+		gpios = <&gpio6 21 GPIO_ACTIVE_LOW>;
+		status = "okay";
+	};
 };
 
 &can0 {
